<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="pit">

	<select id="execSpLogin" statementType="CALLABLE" resultType="UsuarioIbatis">
		call sp_login ( #{vUsuario}, #{vClave} )		
	</select>
	
	<!-- <resultMap type="java.util.LinkedHashMap" id="resultEnlaces"></resultMap> -->
	<select id="execSpEnlaces" statementType="CALLABLE" resultType="EnlaceIbatis">
		call sp_cargarEnlaces ( #{vRol}, #{vOpcion} )
	</select>

</mapper>